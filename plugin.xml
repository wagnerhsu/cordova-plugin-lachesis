<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-lachesis" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>Lachesis</name>
    <js-module name="Lachesis" src="www/Lachesis.js">
        <clobbers target="lachesis" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="Lachesis">
                <param name="android-package" value="cordova-plugin-lachesis.Lachesis" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/com/wagner/lachesis/Lachesis.java" target-dir="src/cordova-plugin-lachesis/Lachesis" />
    </platform>
</plugin>